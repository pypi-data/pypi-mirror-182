"write your own commands"


from opr import Wd, scan, wait
from operbot import cmds, irc, rss


scan(cmds)
scan(irc)
scan(rss)


Wd.workdir = "/var/lib/operbot/"


def main():
    irc.init()
    rss.init()
    wait()


main()
