## configure ##
SMSCONFIG_ENABLE=
JASMIN_CLI_HOST=
JASMIN_CLI_PORT=
JASMIN_CLI_BUFFER_TIME=500000
JASMIN_CLI_USERNAME=jcliadmin
JASMIN_CLI_PASSWORD=jclipwd
JASMIN_CLI_SHARED_TEMP=/tmp/

## biller ##
SMSBILL_ENABLE=

## logger ##
SMSLOG_ENABLE=

## MongoDB ##
MONGODB_CONNECTION_STRING=mongodb://${MONGODB_USERNAME:?err}:${MONGODB_PASSWORD:?err}@${MONGODB_CLUSTER_HOSTS:?err}/?authSource=admin&replicaSet=rs
MONGODB_LOGS_DATABASE=logs
MONGODB_LOGS_COLLECTION=sms_log
MONGODB_MODULES_DATABASE=modules
MONGODB_BILL_DATABASE=modules
MONGODB_BILL_COLLECTION=user
MONGODB_BILL_BALANCE_KEY=mt_messaging_cred quota balance


AMQP_BROKER_HOST=rabbit-mq
AMQP_BROKER_PORT=5672
